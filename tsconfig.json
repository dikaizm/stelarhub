{
    "compilerOptions": {
        "allowJs": true,
        "module": "ESNext",
        "moduleResolution": "bundler",
        "jsx": "react-jsx",
        "strict": true,
        "isolatedModules": true,
        "target": "ESNext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "paths": {
            "@/*": ["./resources/js/*"],
            "@sass/*": ["./resources/sass/*"],
            "@assets/*": ["./resources/assets/*"]
        }
    },
    "include": ["resources/js/**/*.ts", "resources/js/**/*.tsx", "resources/js/**/*.d.ts", "resources/js/Pages/Auth/VerifyEmail.bak", "resources/js/Pages/Auth/ResetPassword.bak", "resources/js/Pages/Auth/Register.bak", "resources/js/Pages/Auth/Login.bak", "resources/js/Pages/Auth/ForgotPassword.bak", "resources/js/Pages/Auth/ConfirmPassword.bak"]
}
